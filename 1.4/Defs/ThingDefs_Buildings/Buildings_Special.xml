<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BuildingBase">
    <defName>AnimaSapling</defName>
    <label>anima sapling</label>
    <description>An anima tree sapling sprouted on the back of an anima bear and now removed from there. Spreads psychic sooth in range.</description>
    <graphicData>
      <texPath>Item/AnimaSapling/AnimaSapling</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(2, 2)</drawSize>
      <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
        <offset>(0.0, 0.0, -0.1)</offset>
      </shadowData>
      <damageData>
        <rect>(0,0.75,1.0,0.5)</rect>
      </damageData>
    </graphicData>
    <uiIconScale>0.85</uiIconScale>
    <thingCategories>
      <li>BuildingsSpecial</li>
    </thingCategories>
    <comps>
      <li Class="CompProperties_MoteEmitter">
        <mote>Mote_PsychicEmanatorEffect</mote>
        <emissionInterval>2000</emissionInterval>
      </li>
    </comps>
    <altitudeLayer>Building</altitudeLayer>
    <building>
      <destroySound>BuildingDestroyed_Metal_Medium</destroySound>
    </building>
    <statBases>
      <MarketValue>1000</MarketValue>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0</Flammability>
      <Beauty>0</Beauty>
      <Mass>15</Mass>
      <WorkToBuild>2000</WorkToBuild>
    </statBases>
    <fillPercent>0.2</fillPercent>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <pathCost>30</pathCost>
    <passability>PassThroughOnly</passability>
    <rotatable>false</rotatable>
    <canOverlapZones>false</canOverlapZones>
    <minifiedDef>MinifiedThing</minifiedDef>
    <specialDisplayRadius>10</specialDisplayRadius>
    <tradeability>None</tradeability>
    <tickerType>Normal</tickerType>
	<designationCategory>Misc</designationCategory>
    <tradeTags>
      <li>UltraExoticBuilding</li>
    </tradeTags>
    <costList>
      <AnimaBranch>1</AnimaBranch>
    </costList>
  </ThingDef>

</Defs>
